<Window x:Class="Proyecto_Negocios_IIP.HistorialdeEstacionamiento"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto_Negocios_IIP"
        mc:Ignorable="d"
        Title="HistorialdeEstacionamiento" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>


        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
            

        </Grid.RowDefinitions>
        <Label Content="Listado de Automoviles En Estacionamiento" Grid.Column="0" Grid.Row="0"/>
        <ListBox Name="lbAutomoviles" Grid.Row="1" Margin="32,5,38,0"/>
        <Button BorderThickness="4"  BorderBrush="#A3E8A5" Background="#92E194"  Content="Historial Automovil" Grid.Column="0" Grid.Row="3" Click="btnhistorialAutomovil_Click" FontSize="18" Margin="68,10,54,10" />
    </Grid>
</Window>
